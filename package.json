{
  "name": "dollidb",
  "version": "1.0.2",
  "main": "build/main.min.js",
  "keywords": [
    "dynamodb"
  ],
  "license": "MIT",
  "scripts": {
    "prepare": "npm run test && npm run prod:build",
    "dev:build": "webpack --config ./webpack/dev.js --watch",
