  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "mocha": "^3.4.2",
    "mocha-webpack": "^0.7.0",
    "webpack": "^2.3.2"
  },
  "dependencies": {
    "aws-sdk": "^2.39.0",
    "chai": "^4.1.0",
    "deep-extend": "^0.4.1",
    "immutable": "4.0.0-rc.2",
    "uuid": "^3.0.1"
  }
}
